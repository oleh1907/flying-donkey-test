{
  //configurable parameters section
  "AppDetails": {
    "AllowedFileTypes": ".txt,.docx,.pdf",
    "AllowedFileSizeInMb": "100000"
  },
  "ConnectionStrings": {
    "FileServerDb": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=FileServerDb;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
